{
    "$schema": "https://frontmatter.codes/config/taxonomy.contenttypes.schema.json",
    "$id": "https://github.com/dgmjr-io/.frontmatter/blob/main/config/taxonomy/contenttypes/analyzerReleases.json",
    "title": "Analyser Releases",
    "name": "AnalyzerReleases",
    "description": "A list of the diagnostics which a code analyzer might emit",
    "pageBundle": false,
    "previewPath": null,
    "fields": [
        {
            "title": "Title",
            "name": "title",
            "type": "string",
            "single": true,
            "multiple": false,
            "required": true,
            "default": "AnalyzerReleases.Shipped.md"
        },
        {
            "title": "Publish date",
            "name": "date",
            "type": "datetime",
            "default": "{{now}}",
            "isPublishDate": true,
            "single": true,
            "multiple": false,
            "required": true
        },
        {
            "title": "Last Modified Date",
            "name": "lastmod",
            "type": "datetime",
            "default": "{{now}}",
            "isModifiedDate": true,
            "single": true,
            "multiple": false,
            "required": true
        }
    ],
    "template": "[[workspace]]/.frontmatter/templates/AnalyzerReleases.md",
    "defaultFleName": "LICENSE.md"
}
