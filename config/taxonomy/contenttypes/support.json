{
  "$id": "https://github.com/dgmjr-io/.frontmatter/blob/main/config/taxonomy/contenttypes/support.json?raw=true",
  "$schema": "https://frontmatter.codes/config/taxonomy.contenttypes.schema.json",
  "defaultFileName": "SUPPPORT.md",
  "description": "A file showing how to get support",
  "fields": [ {
    "description": "The title of the readme file",
    "name": "title",
    "required": true,
    "single": true,
    "title": "Title",
    "type": "string"
  }, {
    "description": "A short description of the project",
    "name": "description",
    "required": true,
    "single": false,
    "title": "Description",
    "type": "string"
  }, {
    "dataFileId": "authors",
    "dataFileKey": "slug",
    "dataFileValue": "name",
    "multiple": true,
    "name": "authors",
    "title": "Authors",
    "type": "dataFile"
  }, {
    "name": "lessImportantMetadata",
    "title": "Less important metadata",
    "type": "divider"
  }, {
    "default": "{{now}}",
    "isPublishDate": true,
    "name": "date",
    "required": true,
    "single": true,
    "title": "Publish date",
    "type": "datetime"
  }, {
    "default": "{{now}}",
    "isModifiedDate": true,
    "name": "lastmod",
    "required": true,
    "single": true,
    "title": "Last Modified Date",
    "type": "datetime"
  }, {
    "name": "draft",
    "title": "Is in draft",
    "type": "draft"
  }, {
    "name": "categories",
    "title": "Categories",
    "type": "categories"
  }, {
    "default": "{{slug}}",
    "name": "slug",
    "required": true,
    "single": true,
    "title": "Slug",
    "type": "slug"
  }, {
    "choices": [ "agpl-3.0", "apache-2.0", "artistic-2.0", "bsl-1.0", "cc0-1.0", "epl-2.0", "gpl-3.0", "isc", "lgpl-3.0", "mit", "mpl-2.0", "ncsa", "nokia", "ofl-1.1", "osl-3.0", "sugarcrm-1.1.3", "unlicense", "wtfpl" ],
    "name": "license",
    "title": "License",
    "type": "choice"
  } ],
  "fileType": "md",
  "name": "support",
  "pageBundle": false
}