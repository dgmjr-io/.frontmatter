{
    "$schema": "https://frontmatter.codes/config/taxonomy.contenttypes.schema.json",
    "$id": "https://raw.githubusercontent.com/dgmjr-io/.frontmatter/main/config/content/types/license.json",
    "title": "License",
    "name": "license",
    "description": "A license for a project",
    "pageBundle": false,
    "previewPath": null,
    "fields": [
        {
            "title": "Title",
            "name": "title",
            "type": "string",
            "single": true,
            "multiple": false,
            "required": true
        },
        {
            "title": "Publish date",
            "name": "date",
            "type": "datetime",
            "default": "{{now}}",
            "isPublishDate": true,
            "single": true,
            "multiple": false,
            "required": true
        },
        {
            "title": "Last Modified Date",
            "name": "lastmod",
            "type": "datetime",
            "default": "{{now}}",
            "isModifiedDate": true,
            "single": true,
            "multiple": false,
            "required": true
        },
        {
            "title": "License",
            "name": "license",
            "type": "dataFile",
            "dataFileId": "licenses",
            "dataFileKey": "licenseId",
            "dataFileValue": "name",
            "single": true,
            "multiple": false
        }
    ],
    "template": "[[workspace]]/.frontmatter/templates/LICENSE.md",
    "defaultFleName": "LICENSE.md"
}
