{
  "$schema": "https://frontmatter.codes/config/data.types.schema.json",
  "$id": "https://github.com/dgmjr-io/.frontmatter/blob/main/config/data/types/project.json",
  "id": "project",
  "schema": {
    "title": "Project",
    "type": "object",
    "required": [
      "name",
      "slug",
      "title",
      "repoUrl"
    ],
    "properties": {
      "name": {
        "type": "string",
        "title": "Name"
      },
      "title": {
        "type": "string",
        "title": "Title"
      },
      "description": {
        "type": "string",
        "title": "Description"
      },
      "slug": {
        "type": "string",
        "title": "slug"
      },
      "repoUrl": {
        "type": "string",
        "title": "Repository URL"
      }
    }
  }
}
