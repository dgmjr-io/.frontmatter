{
  "$id": "https://github.com/dgmjr-io/.frontmatter/blob/main/config/content/types/readme.json",
  "$schema": "https://frontmatter.codes/config/taxonomy.contenttypes.schema.json",
  "frontMatter.taxonomy.contentTypes": [ {
    "defaultFleName": "README.md",
    "fields": [ {
      "multiple": false,
      "name": "title",
      "required": true,
      "single": true,
      "title": "Title",
      "type": "string",
      "wysiwyg": true
    }, {
      "multiple": false,
      "name": "project",
      "required": true,
      "single": true,
      "title": "Project",
      "type": "string"
    }, {
      "multiple": false,
      "name": "description",
      "required": true,
      "single": true,
      "title": "Description",
      "type": "string",
      "wysiwyg": true
    }, {
      "dataFileId": "authors",
      "dataFileKey": "Title",
      "multiple": true,
      "required": true,
      "title": "Authors",
      "type": "dataFile"
    }, {
      "default": "{{now}}",
      "isPublishDate": true,
      "multiple": false,
      "name": "date",
      "required": true,
      "single": true,
      "title": "Publish date",
      "type": "datetime"
    }, {
      "default": "{{now}}",
      "isModifiedDate": true,
      "multiple": false,
      "name": "lastMod",
      "required": true,
      "single": true,
      "title": "Last Modified Date",
      "type": "datetime"
    }, {
      "multiple": false,
      "name": "preview",
      "required": false,
      "single": true,
      "title": "Content preview",
      "type": "image",
      "wysiwyg": false
    }, {
      "name": "draft",
      "title": "Is in draft",
      "type": "draft"
    }, {
      "name": "tags",
      "title": "Tags",
      "type": "tags"
    }, {
      "name": "categories",
      "title": "Categories",
      "type": "categories"
    }, {
      "choices": [ "MIT", "Apache 2.0", "LGPL-3.0", "MPL-2.0", "AGPL-3.0", "Unlicense", "GPL-3.0" ],
      "name": "license",
      "single": true,
      "title": "License",
      "type": "choice"
    } ],
    "name": "readme",
    "pageBundle": false,
    "previewPath": null,
    "template": "[[workspace]]/.frontmatter/templates/README.md"
  } ]
}